<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="INSERT_OR_UPDATE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Display the Base Url field option onLoad from system property..</description>
        <field>api_token</field>
        <global>false</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>vRAGetUrl</name>
        <order/>
        <script><![CDATA[function onLoad() {
	
	
	var ga = new GlideAjax('vRABaseUrlCheck');
	ga.addParam('sysparm_name','fetchBaseUrl');
	ga.getXML(processresp);
	
	
	function processresp(response)
	
	{
		var answer = response.responseXML.documentElement.getAttribute("answer");
		//alert(answer);
		answer=answer.replace(/,\s*$/, "");
		var url_arr=answer.split(',');
		for(var i=0;i<url_arr.length;i++)
		
		{
			g_form.addOption('cas_url',url_arr[i]+'',url_arr[i]+'');
		}
	}
}


]]></script>
