        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	//entitlement gliderecord query
	var entGR = new GlideRecord('x_vmw_cloudservice_cas_catalog_entitlement');
	entGR.addEncodedQuery('select_projects='+current.sys_id+'');
	entGR.query();
	while(entGR.next()){
		var projects=entGR.select_projects+'';
		var projects_arr=projects.split(',');
		var index=projects_arr.indexOf(current.sys_id+'');
		if(index>-1){
			projects_arr.splice(index,1);
		}
		projects=projects_arr.toString();
		entGR.select_projects=projects;
		
		//updating catalog items field
		var catItemGR = new vRAOnChangeHideItems();
		entGR.select_catalog_items = catItemGR.updateItems(projects,entGR.select_catalog_items,'x_vmw_cloudservice_cas_catalog_items','category');
		entGR.update();
	}
})(current, previous);]]></script>
