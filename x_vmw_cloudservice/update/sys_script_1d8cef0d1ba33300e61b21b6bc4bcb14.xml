        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	try{
		CASLogger.debug("Inside BR MakeRelationshipDeploymentProject for Making Relationship with project");
		this.appUtil=new CASAppUtil();
		var gr = new GlideRecord(this.appUtil.configuration.table.project);
		gr.addQuery("project_id",current.u_project.project_id);
		gr.query();
		if(gr.next()) {
			current.u_project=gr.sys_id;
			current.update();
		}
	}
	catch(e)
	{
		CASLogger.error("Exception Caught inside BR MakeRelationshipDeploymentProject "+e);
	}
	
})(current, previous);]]></script>
