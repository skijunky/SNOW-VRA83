        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    try {
        CASLogger.debug('Inside  vRADeleteCatalogWithSameCasUid.Business Rule:Delete catalog with same cas_uid');
        
        this.appUtil = new CASAppUtil();

        var gr = new GlideRecord(this.appUtil.configuration.table.catalogItem);
        gr.addQuery("cas_uid", current.cas_uid);
		gr.addEncodedQuery('cas_uidISNOTEMPTY');
        gr.query();
        if (gr.next()) {
			
			current.setAbortAction(true);
			
        }
    } catch (e) {
		
CASLogger.error("Exception caught inside vRADeleteCatalogWithSameCasUid.Business Rule:" + e);
    
	}


})(current, previous);]]></script>
